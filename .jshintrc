{
  // see https://raw.github.com/jshint/jshint/master/examples/.jshintrc for definitions

  "passfail": false,
  "maxerr": 50,
  "browser": true,
  "node": true,
  "rhino": false,
  "couch": false,
  "wsh": false,
  "jquery": true,
  "prototypejs": false,
  "mootools": false,
  "dojo": false,
  "predef": [
    // angular
    "angular",
    "pause",
    "sleep",
    "browser",
    "expect",
    "using",
    "binding",
    "input",
    "repeater",
    "select",
    "element",

    // jasmine
    "jasmine",
    "isCommonJS",
    "exports",
    "spyOn",
    "it",
    "xit",
    "expect",
    "runs",
    "waits",
    "waitsFor",
    "beforeEach",
    "beforeAfter",
    "describe",
    "xdescribe",
    "nonew",
    "unused"
  ],
  "debug": false,
  "devel": false,
  "es5": false,
  "strict": false,
  "globalstrict": false,
  "asi": false,
  "bitwise": true,
  "boss": false,
  "curly": true,
  "eqeqeq": true,
  "eqnull": false,
  "esnext": true,
  "evil": false,
  "expr": false,
  "forin": true,
  "funcscope": false,
  "immed": true,
  "iterator": false,
  "latedef": true,
  "lastsemic": false,
  "laxbreak": false,
  "laxcomma": false,
  "loopfunc": false,
  "multistr": false,
  "nonstandard": false,
  "onecase": false,
  "proto": false,
  "noarg": true,
  "regexp": true,
  "regexdash": false,
  "scripturl": false,
  "shadow": false,
  "smarttabs": false,
  "supernew": false,
  "undef": true,
  "validthis": false,
  "indent": 2,
  "newcap": true,
  "noempty": false,
  "nomen": false,
  "onevar": false,
  "plusplus": false,
  "sub": false,
  "trailing": true,
  "white": false
}
